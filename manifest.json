{
    "manifest_version": 3,
    "name": "Domain ICP Info",
    "version": "1.0",
    "description": "Get the ICP info of a domain.",
    "permissions": ["activeTab"],
    "host_permissions": [ "http://*/*", "https://*/*"],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }